function imageCache(el, binding,type){
  if (window.api){
    setTimeout(function() {
      window.api.imageCache({
        url: getFullPath(binding.value),
        thumbnail: false
      }, function (ret) {
        if (ret) {
          if(type=='img'){
            el.setAttribute("src", ret.url);
            console.log(ret.url)
          }else{
            el.setAttribute("style", {'background-image':'url('+(ret.url)+')'});
          }
        }
      });
    });
  }else{
    setTimeout(()=>{
      imageCache(el, binding);
    },500)
  }
}
