  border: 1px solid #d0cdd1;
  color:#d0cdd1;
}
//橙色字体
.orange_text{
  color:#ffa443;
}
//灰色字体
.gray_text{
  color:#d0cdd1 !important;
}
//灰色隔间
.space_gray{
  width: 100%;
  height: 0.3rem;
  background-color: #F2F2F2;
}
.border-bottom{
  border-bottom: 1px solid #F2F2F2;
}
.show_hide_text{
  color: #667592;
}
ul.scroll_title{
  overflow: hidden !important;
  min-width: 100%;
  @extend %contentWidth;
  li{
    float: left;
    position: relative;
    padding: 0 0.3rem;
    height: 1.2rem;
    line-height: 1.2rem;
    background-color: #FFFFFF;
    &::before {
      content: "";
      position: absolute;
      bottom: 6px;
      left: 100%;
      width: 0;
      height: 0;
      border-bottom: 2px solid #ff4300;
      transition: 0.2s all linear;
    }
  }
  li.active {
    &::before {
      width: 0.81rem;
      left: 30%;
      transition-delay: 0.1s;
    }
    color: #000;
  }
  li.active ~ li {
    &::before {
      left: 0;
    }
  }
}
.paddingBottom2{
  padding-bottom: 2rem !important;
}
.paddingTop15{
  padding-top: 1.5rem;
}
.loading{
    position:fixed;width:100%;z-index:989;height:100%;left:0;top:0;color:#fff;
    i{
        position:absolute;top:50%;left:50%;width:1.6rem;height:1.6rem;margin:-0.8rem 0 0 -0.8rem;text-align:center;line-height:1.6rem;@include bgcolor(#000,0.5);border-radius:0.3rem;
        &:before{content:"加载中...";font-size:0.24rem;}
    }
    >div{
        position:absolute;top:50%;left:50%;width:1.6rem;height:1.6rem;margin:-0.8rem 0 0 -0.8rem;text-align:center;line-height:1.6rem;@include bgcolor(#000,0.5);border-radius:0.3rem;
    }
    .loadtime{
        font-size:0.24rem;
    }
    .loadtext{
        position:absolute;left:-100%;top:100%;width:300%;line-height:1.5; margin-top:0.1rem;font-size:0.3rem;text-align:center;
		em{
			display:inline-block;vertical-align:top;@include bgcolor(#000,0.5);border-radius:0.5rem;padding:0.02rem 0.15rem 0;
		}
    }
    svg {
        position:absolute;left:20%;top:20%;
        -webkit-animation:spin 2s linear infinite;
        -moz-animation:spin 2s linear infinite;
        animation:spin 2s linear infinite;
        width:60%;
        height:60%;
        fill: #fff;
    }
}
@-webkit-keyframes spin { 100% { -webkit-transform: rotate(-360deg); } }
@-moz-keyframes spin { 100% { -moz-transform: rotate(-360deg); } }
@keyframes spin { 100% { -webkit-transform: rotate(-360deg); transform:rotate(-360deg); } }
%clearFix{
    *zoom:1;
    &:after{
        content:"";display: block; height: 0; clear: both; visibility: hidden;
    }
}
%active{
    &:active{
        background-color:#e5e5e5;
    }
}
%ban{
    &:before{content:"";position:absolute;left:0;bottom:0;z-index:1;width:100%;height:0.25rem;background:url(../images/ban.png) no-repeat;background-size:100% 100%;@include translateZ(0);}
}
%leftImg{
    display:block;float:left;@include fixbackground;
}
@mixin triangle($width:0.1rem,$color:#e22426,$rotate:135deg) {
    width:$width;height:$width;border-style:solid;border-color:$color $color transparent transparent;border-width:1px 1px 0 0;@include rotate($rotate);
}
%oneLine{
    overflow: hidden;text-overflow: ellipsis; white-space: nowrap;
}
%unoneLine{
    text-overflow: initial; white-space: initial;
}
%contentWidth{width:-moz-max-content;width:-ms-max-content;width:-webkit-max-content;width:max-content;}
%iconfont{font-family: "iconfont" !important; font-style: normal;-webkit-font-smoothing: antialiased;-moz-osx-font-smoothing: grayscale;}
body{
	&.fullscreen{
        overflow:hidden;position:absolute;left:0;top:0;width:100%;height:100%;
		.main{height:100%;}
    }
}
@-webkit-keyframes weuiLoading{
	0%{-webkit-transform:rotate(0deg);transform:rotate(0deg)}to{-webkit-transform:rotate(1turn);transform:rotate(1turn)}
}
@keyframes weuiLoading{
	0%{-webkit-transform:rotate(0deg);transform:rotate(0deg)}to{-webkit-transform:rotate(1turn);transform:rotate(1turn)}
}
.main{
	.myscroller{
        position:fixed;left:0;top:0;height:100%;width:100%;@include box-sizing;touch-action: none;overflow:hidden;
        >div{
            position:relative;height:100%;overflow:hidden;@include box-sizing;//-webkit-overflow-scrolling:touch;
        }
        &.highscroll{
            position:relative;
            >div{
                padding:0!important;
            }
        }
        ._v-container{
            position:relative;
            >._v-content{
                >.pull-to-refresh-layer{
                    font-size:0.4rem;
                }
                >.loading-layer{
                    font-size:0.4rem;
                }
            }
        }
		.spinner-holder.active,.pull-to-refresh-layer.active{
			.myScroller_loading{
				display: inline-block;-webkit-animation-play-state: running;animation-play-state: running;
			}
		}
		.myScroller_loading{
			width:30px!important;height:30px!important;display: none;vertical-align:top;margin:15px auto 0;@include animation('weuiLoading 1s steps(12, end) infinite');background: transparent url("data:image/svg+xml;base64,") no-repeat; background-size: 100%;
		}
    }
	#tinymce_ifr,.mce-tinymce{
		height:100%!important;overflow:hidden;
	}
	.mce-edit-area{
		height:100%;overflow:hidden;
	}
	.mce-tinymce>div{
		position:absolute;left:0;top:0;width:100%;height:100%!important;overflow:hidden;
	}
	.mce-top-part{
		padding:0.2rem;position:fixed;z-index:1;left:0;bottom:0;width:100%;height:1.71rem;background-color:#fff;border-top:1px solid #F2F2F2;@include box-sizing;
	}
	.mce-edit-area{
		height: 100%;position: absolute;left: 0;top: 0;width: 100%;@include box-sizing;
	}
	.mce-btn-group{
		position:relative;z-index:0;display:inline-block;margin-left:0.1rem;width::1.2rem;height:1.2rem;line-height::1.2rem;
		.mce-btn{
			background:none;
		}
		&:before{
			content:"\e663";position:absolute;left:0;top:0;width:100%;height:100%;@extend %iconfont;color:#C0C0C0;text-align:center;font-size:0.6rem;background-color:#F2F2F2;z-index:-1;
		}
	}
	/*编辑器*/
	.ql-editor{
		padding:0.15rem;font-size:0.4rem;color:#000;
		p{margin:0.2rem 5%;}
		img{display:block;max-width:90%!important;margin:0.2rem auto;}
	}
	.quill-editor{
		@include box-sizing;max-height:100%;min-height:100%;overflow-x:hidden;overflow-y:auto;-webkit-overflow-scrolling:touch;
		-webkit-touch-callout:text!important;
		-webkit-user-select:text!important;
		-khtml-user-select:text!important;
		-moz-user-select:text!important;
		-ms-user-select:text!important;
		user-select:text!important;
	}
	.ql-toolbar.ql-snow,.ql-container.ql-snow{
		border:0;
	}
	.ql-toolbar.ql-snow{
		display:none;position:fixed;left:0;padding:0.1rem;bottom:0;width:100%;background-color:#fff;z-index:1;border-top:1px solid #F2F2F2;
		.ql-image{
			position:relative;width:1.2rem;height:1.2rem;line-height:1.2rem;
			>svg{
				display:none;
			}
			&:before{
				content:"\e663";position:absolute;left:0;top:0;width:100%;height:100%;@extend %iconfont;color:#C0C0C0;text-align:center;font-size:0.6rem;background-color:#F2F2F2;z-index:-1;
			}
		}
	}
}
.gray {
  background-color: #F5F5F5;
}
.lgray{
	background-color:#F8F8F8;
}
.jy_all_top{
	padding:2.14rem  0 0;
	&:before{background-color:#3a3845 ;z-index:600;}
	.header{
		position:fixed;z-index:400;left:0;top:0.75rem;width:100%;
		.top{
			background-color:#3a3845 ;color:#FFF;font-size: 0.52rem;line-height:1.2;position: relative;
			i{font-size: 0.52rem;}
			>a{position:absolute;color:#fff;top:0.4rem;}
			.left{left:0.325rem;}
			.right{
				right:0.325rem;color:#000;
				&.right2{
					right:1.3rem;
				}
			}
			
			p{margin:0 0.8rem;@extend %oneLine;}
		}
	}
}

