	0% {left: 0;top: 50%;}
	41% {left: 41%;top: 50%;}
	44.6% {left: 44.6%;top: 38%;}
	51.4% {left: 51.4%;top: 66%;}
	58% {left: 58%;top: 27%;}
	62% {left: 62%;top: 50%;}
	to {left: 110%;top: 50%;}
}
@keyframes xinz_ain {
	0% {transform: scale(1);}
	41% {transform: scale(1);}
	44.6% {transform: scale(1.05);}
	51.4% {transform: scale(.95);}
	58% {transform: scale(1.05);}
