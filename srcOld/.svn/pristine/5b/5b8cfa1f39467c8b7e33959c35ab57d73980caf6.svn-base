    }

    p {
      margin: 0;
      font-size: 0.32rem;
      color: #adadad;
    }

    p:not(.tabs_p) {
      &:before {
        content: "●";
        color: #E0E0E0;
        font-size: 0.24rem;
        line-height: 2;
        vertical-align: top;
        display: inline-block;
        margin-right: 0.6rem;
        margin-left: 0.38rem;
      }
    }

    .tabs_p {
      font-size: 0.4rem;
      height: 0.53rem;
      line-height: 0.53rem;
      color: #000;
      span {
        display: inline-block;
      }
    }

    div {
      position: absolute;
      bottom: 0;
    }

    & > span {
      position: absolute;
      right: 0;
      font-size: 0.28rem;
      display: inline-block;
      width: 1.32rem;
      height: 0.4rem;
      line-height: 0.4rem;
      text-align: center;
    }

    & > span:nth-of-type(1) {
