body {
  font-size: 14px;
  line-height: 1.5;
  font-family: "Microsoft Yahei", Arial, Helvetica, sans-serif, "宋体";
  -webkit-text-size-adjust: none;
  -webkit-font-smoothing: antialiased;
  .pswp {

    .pswp__top-bar {
      top: 0.42rem;
    }
  }
  .weui-toast {
    min-width: 50%;
    word-break: keep-all;
    padding: 0 0.1rem;
    &.vux-toast-bottom {
		top:auto!important;
      bottom: 1.74rem;
    }
  }
  .weui-mask {
    position: fixed !important;
    z-index: 1000 !important;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
  }
  .weui-icon_toast {
    margin: 0.2rem 0 0.1rem;
  }

}

a {
  text-decoration: none;
}

ul, li, p, h1, h2, h3, h4, h5, dl, dt, dd, ol, body {
  padding: 0;
  margin: 0;
}

ul, li {
  list-style: none;
}

img {
  border: 0;
}

input,textarea {
	-webkit-user-select: text;
	margin: 0;
	padding: 0;
	outline: none;
}
input {
  margin: 0;
  padding: 0;
  border-radius: 0;
  border: 0;
  &:focus {
    outline: none;
  }
}

i, em {
  font-style: normal;
}

