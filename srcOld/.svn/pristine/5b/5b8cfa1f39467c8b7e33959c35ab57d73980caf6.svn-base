.zmjy-ul {
  overflow: hidden !important;
  min-width: 100%;
  @extend %contentWidth;
  li {
    float: left;
    margin-right: 0.26rem;
    a {
      display: block;
      width: 3.14rem;
      height: 3.14rem;
      background-size: 100% 100%;
      background-repeat: no-repeat;
      position: relative;
      i {
        font-size: 0.28rem;
        position: absolute;
        opacity: 0.5;
        filter: progid:DXImageTransform.Microsoft.Alpha(opacity=60);
        padding: 0.05rem;
        color: #FFF;
        background-color: rgba(0, 0, 0, 0.5);
        &.count {
          display: -webkit-box;
          display: -ms-flexbox;
          display: flex;
          -webkit-box-pack: center;
          -ms-flex-pack: center;
          justify-content: center;
          -webkit-box-align: center;
          -ms-flex-align: center;
          align-items: center;
          border-radius: 2px;
          top: 0.1rem;
          right: 0.072rem;
        }
        &.name {
          width: 97%;
          left: 0;
          bottom: 0;
          border-bottom-left-radius: 0.06rem;
          border-bottom-right-radius: 0.06rem;
          -moz-border-radius-bottomleft: 0.06rem;
          -moz-border-radius-bottomright: 0.06rem;
        }
      }
    }
    p {
      width: 3.14rem;
      display: flex;
      justify-content: space-between;
      align-items: center;
      position: relative;
      margin: 0.24rem 0 0;
      .icon_zs {
        position: absolute;
        left: 0;
        width: 1rem;
        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
      }
      span {
        font-size: 0.36rem;
        background-color: #ff84ac;
        color: #FFF;
        border-radius: 3px;
        margin-left: 0.05rem;
        padding: 0 0.22rem;
        white-space: nowrap;
      }
      i {
        font-size: 0.28rem;
        margin-right: 0.05rem;
      }

    }
  }
}

.img_list {
