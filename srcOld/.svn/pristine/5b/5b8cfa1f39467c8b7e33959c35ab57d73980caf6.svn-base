      font-size: 0.32rem;
      margin-left: 0.9rem;
      margin-right: 0;
    }

    .icon_zan {
      background-image: url(../images/icon_zan.png);
    }

    .icon_msg {
      background-image: url(../images/icon_msg.png);
    }

    .icon_flower {
      background-image: url(../images/icon_flower.png);
    }

    .icon_comment {
      background-image: url(../images/icon_comment.png);
    }
  }
  .share_a{
    width: 10%;
    position: absolute;
    right: -0.6rem;
    top: 0.3rem;
    padding: 2px;
  }
