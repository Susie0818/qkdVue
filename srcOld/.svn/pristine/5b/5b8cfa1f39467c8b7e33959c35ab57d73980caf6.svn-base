	.jy_act_bl_tip{
		top:auto!important;bottom:0;
		i{
			float:right;color:#fff;font-size:0.32rem;
			&.jy_act_newPeople{
				&:before{
					content:"\e6db";margin-right:0.05rem;@extend %iconfont;
				}
			}
		}
		a{
			color:#00d2ff;
		}
	}
	.jy_act_top{
		overflow:hidden;padding:2.1rem 0 0.7rem;

	}
	.jy_act_noPrize{
		text-align:center;color:#fff;font-size:0.4rem;padding:0.2rem;
	}
	.jy_act_mid{
		padding:6.45rem 0 0.9rem;
		&.jy_act_mid_manage{
			padding:6.45rem  0 0;
		}
		&.jy_act_mid_dia{
			padding:6.45rem  0 1.44rem;
		}
	}
	.jy_act_bl_scroll {
		padding-top:1rem;
		&.on{
			padding-top:2rem;
		}
	}
	.jy_bd_menu{
		position:absolute;left:0;top:0;width:100%;@include bgcolor(#000,0.2);
		&.cur{
			i{
				width:16.66667%;
			}
		}
		i{
			position:relative;display:block;height:0.9rem;line-height:0.9rem;width:20%;float:left;text-align:center;color:#fff;font-size:0.36rem;@extend %iconfont;
			&.cur{
				background-color:#ffe200;color:#000;
			}
			&.on{
				&:before{
					content:"";background-color:#f00;position:absolute;width:0.2rem;height:0.2rem;border-radius:50%;right:0.1rem;top:0.1rem;font-size:0.3rem;
				}
			}
		}
	}
	.jy_act_mid_top{
		position:absolute;left:0;top:1.05rem;padding:0 0.2rem;width:100%;line-height:0.8rem;color:#fff;font-size:0.4rem;@include box-sizing;
		.jy_bl_jy_act_mid_top_r{
			float:right;
			i{
				@extend %iconfont;padding:0 0.1rem;vertical-align:top;
			}
		}
		.jy_bl_Choose_search{
			width:8rem;
			input{
				display:block;touch-action:none;width:100%;font-size:0.3rem;color:#CDD2CB;border-radius:2rem;@include bgcolor(#000,0.25);padding:0.23rem 0.1rem;text-align:center;
				&::-webkit-input-placeholder{color:#CDD2CB;}
			}
		}
	}
	.jy_act_bottom2{
		padding:0 0.2rem;height:0.9rem;position:absolute;width:100%;left:0;bottom:0;@include box-sizing;@include bgcolor(#fff,0.1);
		@at-root .jy_act_bd_btn{
			float:left;font-size:0.44rem;line-height:0.9rem;color:#fff;
			&.cur{
				i{
					background-color:#FFCC00;
					&:before{
						background-color:#FFCC00;
					}
					&:after{
						left:55%;
					}
				}

			}
			&:last-child{
				float:right;
			}
			i{
				float:left;margin:0.15rem 0.1rem 0 0;position:relative;border-radius:2rem;display:inline-block;width:1.3rem;height:0.6rem;@include box-sizing;background-color:#E5E5E5;@include transition(background-color,0.3s,ease);
				&:before{
					content:"";position:absolute;left:5%;top:10%;height:80%;width:90%;border-radius:1rem;background-color:#fff;@include transition(background-color,0.3s,ease);
				}
				&:after{
					content:"";position:absolute;left:5%;top:50%;width:0.5rem;height:0.5rem;margin-top:-0.25rem;background-color:#fff;box-shadow:0 -0.03rem 0.2rem -0.05rem #000;border-radius:50%;@include transition(left,0.3s,ease);
				}
			}
			em{
				display:inline-block;vertical-align:top;color:#FFC602;@extend %iconfont;margin-left:0.1rem;font-size:0.5rem;
			}
		}
	}
	.s2{
		.jy_act_mid{
			padding:2.75rem 0 0;
		}
	}
	.jy_act_input.input-div {
		background:transparent!important;border-top:0!important;
		.input{
			height:1.44rem!important;line-height:1.44rem!important;
			input{
				@include bgcolor(#fff,0.2);border-radius:0.1rem;color:#fff;padding:0 0.1rem;
			}
			div {
				background-color: #ff0060;color:#fff!important;
				i{
					color:#fff!important;
				}
			}
		}
	}
	.showtip{
		position:absolute;left:0;top:0;height:100%;width:100%;overflow-x:hidden;overflow-y:auto;@include bgcolor(#000,0.8);
		.showtipImg{
			width:100%;height:19.2rem;background:url(../images/jy_act_tc.jpg) no-repeat;background-size:100%;
			i{
				color:#fff;font-size:0.8rem;position:absolute;right:0.3rem;top:0.5rem;@extend %iconfont;padding:0 0.2rem;
			}
		}
	}
}
.jy_act_lt_manage_msg{
	padding:0.2rem 0.5rem;height:100%;@include box-sizing;overflow-x:hidden;overflow-y:auto;-webkit-overflow-scrolling:touch;
	li{
		font-size:0.4rem;color:#000;@include bgcolor(#000,0.3);border-radius:0.1rem;margin-bottom:0.2rem;padding:0.2rem;color:#fff;-webkit-user-select:text;user-select:text;
	}
}
.jy_bd_menu_result{
	padding:0 0.35rem;height:100%;overflow-x:hidden;overflow-y:auto;-webkit-overflow-scrolling:touch;
	li{
		position:relative;overflow:hidden;padding:0.4rem  0 0.4rem 1.15rem;border-bottom:1px solid #B0AEAF;
		&:last-child{
			border:0;
		}
		&:before{
			content:attr(data-num);position:absolute;left:0;top:50%;margin-top:-0.5rem;color:#fff;font-size:0.48rem;width:0.68rem;height:0.68rem;line-height:0.68rem;text-align:center;background-color:#9B9A9F;border-radius:0.1rem;
		}
		.jy_bd_menu_c{
			overflow:hidden;position:relative;z-index:0;width:5rem;
			&.jy_bd_menu_c2{
				width:7rem;
			}
			&.jy_bd_menu_c_blur{
				&:after{
					content:"";z-index:1;position:absolute;left:0;top:0;width:100%;height:100%;@include bgcolor(#000,0.5);border-radius:2rem;
				}
				span{
					border-radius:2rem;overflow:hidden;
					&>*{
						-webkit-filter: blur(0.1rem);filter: blur(0.1rem);
					}
				}
			}
			&:before{
				content:"";position:absolute;left:50%;top:50%;z-index:-1;width:1.4rem;height:0.58rem;margin:-0.6rem 0 0 -0.7rem;background:url(../images/act_xin2.png);background-size:100% 100%;
			}
			.jy_bd_menu_result_type{
				position:absolute;top:50%;left:50%;font-size:0.26rem;width:1.2rem;height:0.46rem;line-height:0.46rem;text-align:center;margin:0.1rem 0 0 -0.6rem;border-radius:1rem;color:#fff;background-color:#FFA800;
				&.cur2{
					background-color:#00AEFF;
				}
				&.cur3{
					background-color:#FF5297;
				}
			}
			span{
				display:block;width:50%;float:left;@include box-sizing;
				&.jy_bd_menu_result_l{
					padding-right:0.5rem;
				}
				&.jy_bd_menu_result_r{
					padding-left:0.5rem;
				}
				i{
					position:relative;overflow:hidden;display:block;margin:0 auto;border-radius:50%;width:1.1rem;height:1.1rem;background-image:url(../images/default_head.gif);@include fixbackground;
					em{
						position:absolute;width:130%;height:100%;left:-15%;top:70%;background-color:#ff0060;font-size:0.24rem;color:#fff;text-align:center;line-height:1.2;border-radius:50%;
						&.cur{
							background-color:#00b4ff;
						}
					}

				}
				p{
					font-size:0.3rem;margin-top:0.1rem;color:#fff;text-align:center;@extend %oneLine;
				}
			}

		}
		.jy_bd_menu_btn{
			position:absolute;right:0;top:50%;width:2.36rem;height:0.88rem;line-height:0.88rem;text-align:center;font-size:0.44rem;color:#000;border-radius:5rem;background-color:#FFE200;margin-top:-0.44rem;
			&.off{
				background-color:#cbcbcb;
			}
		}
	}
}
.jy_dia{
	position:absolute;z-index:9;left:0;top:0;width:100%;height:100%;@include bgcolor(#000,0.4);
	.jy_dia_c{
		position:absolute;left:50%;top:50%;padding:1.5rem 0 1.8rem;width:9.8rem;height:8.8rem;margin:-4.4rem 0 0 -4.9rem;background-color:#fff;border-radius:0.2rem;@include box-sizing;
	}
	.jy_dia_top{
		position:absolute;z-index:1;left:0;top:0;height:1.5rem;line-height:1.5rem;width:100%;border-bottom:1px solid #808080;font-size:0.4rem;color:#808080;overflow:hidden;
		.jy_dia_topImg{display:block;float:left;width:0.8rem;height:0.8rem;border-radius:0.1rem;@include fixbackground;background-image:url(../images/default_head.gif);margin:0.35rem 0.3rem 0 0.5rem;}
		em{
			font-size:0.6rem;@extend %iconfont;float:right;margin-right:0.5rem;
		}
		.jy_act_bd_btn{
			color:#000;float:right;margin:0.3rem 0.5rem 0 0;
		}
		.jy_dia_search{
			padding-left:0.2rem;
			input{
				color:#999;font-size:0.4rem;padding:0.2rem 0.1rem;border:1px solid #f1f1f1;border-radius:0.1rem;width:4rem;@include box-sizing;
			}
		}
	}
	.jy_dia_bottom{
		position:absolute;left:0;bottom:0;height:1.8rem;line-height:0.9rem;width:100%;padding-right:1.6rem;background-color:#F5F5F5;border-radius:0 0 0.2rem 0.2rem;@include box-sizing;
		textarea{
			display:block;width:100%;border:0;height:100%;resize:none;color:#bababa;font-size:0.4rem;padding:0.2rem 0.1rem;border-right:1px solid #ccc;background:#F5F5F5;@include box-sizing;
			&::-webkit-input-placeholder{color:#bababa;}
		}
		i{
			position:absolute;right:0;top:0;font-size:0.4rem;width:1.6rem;text-align:center;color:#bababa;@include box-sizing;padding-top:0.5rem;height:100%;
		}
	}
	.jy_act_lt_list{
		padding:0.2rem 0.5rem;height:100%;@include box-sizing;overflow-x:hidden;overflow-y:auto;-webkit-overflow-scrolling:touch;
		li{
			min-height:1rem;
			.jy_lt_img{
				border-radius:50%;
			}
			.jy_lt_msg{
				background:#E6E6E6;border:0;font-size:0.4rem;max-width:82%;
				&:after{
					display:none;
				}
				&:before{
					border-color:transparent transparent transparent #E6E6E6
				}
			}
		}
	}
	.jy_dia_err{
		position:absolute;width:80%;left:10%;height:4rem;top:50%;margin-top:-2rem;background-color:#fff;border-radius:0.2rem;padding:0.3rem;@include box-sizing;font-size:0.4rem;
		.jy_dia_err_desc{
			text-align:center;margin-top:0.3rem;
		}
		.jy_dia_err_btn{
			position:absolute;left:0;bottom:0;width:100%;text-align:center;border-top:1px #f1f1f1 solid;
			i{
				display:inline-block;padding:0.2rem 0.3rem;width:33.33%;@include box-sizing;border-left:1px #f1f1f1 solid;
				&:first-child{border:0;}
			}
		}
	}
}
.jy_bd_menu_list,.jy_bd_menu_biaobai{
	height:100%;overflow-x:hidden;overflow-y:auto;padding:0.2rem 0.6rem;@include box-sizing;-webkit-overflow-scrolling:touch;
	li{
		overflow:hidden;position:relative;padding:0.3rem 0 0.3rem 1.35rem;border-bottom:1px solid #B0AEAF;
		&:last-child{
			border:0;
		}
		.jy_bd_menu_img{
			position:absolute;top:0.3rem;left:0;overflow:hidden;display:block;float:left;border-radius:50%;width:1.1rem;height:1.1rem;@include fixbackground;margin-right:0.35rem;background-image:url(../images/default_head.gif);
			em{
				position:absolute;width:130%;height:100%;padding-top:0.05rem;left:-15%;top:70%;background-color:#ff0060;font-size:0.24rem;color:#fff;text-align:center;line-height:1.2;border-radius:50%;
			}
		}
		h4{
			color:#fff;font-size:0.4rem;font-weight:normal;
		}
		.jy_auth_age{
			margin:0;
		}
		.jy_bd_menu_num{
			position:absolute;right:0;top:50%;color:#fff;font-size:0.36rem;margin-top:-0.26rem;
			i{
				@extend %iconfont;color:#ff2856;font-size:0.5rem;margin-left:0.1rem;
			}
		}
	}
}
.jy_bd_menu_biaobai{
	li{
		p{font-size:0.36rem;color:#fff448;}
	}
}
.jy_act_top_user{
	overflow:hidden;position:relative;
	.jy_act_top_user_img{
		position:relative;overflow:hidden;display:block;float:left;width:2rem;height:2rem;margin:0 0.45rem 0 1.1rem;@include fixbackground;background-image:url(../images/default_head.gif);border-radius:50%;border:0.05rem solid #FFC002;
		em{
			position:absolute;width:130%;height:100%;padding-top:0.2rem;left:-15%;top:70%;background-color:#ff0060;font-size:0.3rem;color:#fff;text-align:center;line-height:1.2;border-radius:50%;
		}
	}
	.jy_act_top_user_text{
		font-size:0.36rem;color:#fff;line-height:2;margin-top:0.3rem;
		a{
			color:#ffea00;
		}
	}
	.jy_act_step_btn{
		position:absolute;padding:0;right:0.1rem;top:0;width:5rem;
		li{width:25%;float:right;}
	}
}
.jy_act_top_user_tongji{
	clear:both;overflow:hidden;margin:0.5rem 0.4rem 0.2rem;
	li{
		width:33.33%;float:left;font-size:0.48rem;color:#fff;text-align:center;
		em{
			display:block;font-size:0.3rem;
		}
	}
}
.jy_bd_user{
	padding:0 0.1rem;height:100%;overflow-x:hidden;overflow-y:auto;-webkit-overflow-scrolling:touch;
	&.userStyle_1{
		li{
			width:50%;font-size:0.5rem;
			.jy_bd_user_img{
				border-radius:0.2rem;
				em{
					width:1.2rem;height:1.2rem;
					&:before{
						background-size:auto 0.96rem;
					}
				}
				span{
					font-size:0.48rem;line-height:0.9rem;padding:0 0.2rem;color:#fff;border-radius:2rem;@include bgcolor(#000,0.5);
				}
				.chooseMe{
					font-size:0.6rem;
				}
			}
			p{
				padding:0.16rem 1.36rem 0.16rem 0.16rem;font-size:0.5rem;border-radius:0.2rem;
				.jy_auth_age{
					font-size:0.4rem;top:0.1rem;height:0.8rem;line-height:0.8rem;padding:0 0.3rem;
				}
			}

		}
	}
	li{
		position:relative;width:25%;float:left;padding:0 0.1rem;font-size:0.25rem;color:#fff;@include box-sizing;margin-bottom:0.3rem;
		&.cur{
			.jy_bd_user_img{
				&:before{
					content:"已选";position:absolute;left:0;top:0;border-radius:0.1rem;background-color:#f00;font-size:0.3rem;color:#fff;padding:0.1rem;
				}
			}
		}

		.jy_bd_user_img{
			position:relative;display:block;border-radius:0.1rem;padding-top:105%;@include fixbackground;background-image:url(../images/default_head.gif);
			em{
				position:absolute;width:0.6rem;height:0.6rem;border-radius:50%;@include bgcolor(#000,0.3);
				&.cur{
					@include bgcolor(#fff,0.8);
				}
				&:before{
					content:"";position:absolute;left:0;top:0;width:100%;height:100%;background-position:center center;background-repeat:no-repeat;background-size:auto 0.48rem;
				}
			}
			.chooseMe{
				position:absolute;width:100%!important;height:auto!important;background:none!important;left:0;top:20%;text-align:center;color:#f00;font-size:0.5rem;-webkit-text-stroke: 0.02rem #fff;letter-spacing:0.05rem;-webkit-text-fill-color: #f00;font-weight:bold;@include animation('text_ani 0.3s linear alternate infinite');-webkit-animation-play-state: running;animation-play-state: running;
			}
			.jy_bd_choose{
				top:0.05rem;left:0.08rem;
				&:before{
					background-image:url(../images/bd_ico1.png);
				}
				&.cur{
					&:before{
						background-image:url(../images/bd_ico1_h.png);
					}
				}
			}
			.jy_bd_love{
				top:0.05rem;right:0.08rem;
				&:before{
					background-image:url(../images/bd_ico2.png);
				}
				&.cur{
					&:before{
						background-image:url(../images/bd_ico2_h.png);
					}
				}
			}
			.jy_bd_com{
				bottom:0.05rem;right:0.08rem;
				&:before{
					background-image:url(../images/bd_ico3.png);
				}
				&.cur{
					&:before{
						background-image:url(../images/bd_ico3_h.png);
					}
					&:after{
						 content: attr(data-num);position: absolute;right: -0.05rem;top: -0.1rem;width: 0.4rem;height: 0.4rem;border-radius: 50%;background-color: #FE0000;text-align:center;line-height:0.4rem;font-size:0.22rem;
					}
				}
			}
			span{
				position:absolute;left:0.08rem;bottom:0.05rem;font-size:0.24rem;line-height:0.44rem;padding:0 0.2rem;color:#fff;border-radius:2rem;@include bgcolor(#000,0.5);
			}
		}
		p{
			padding:0.08rem 0.68rem 0.08rem 0.08rem;font-size:0.25rem;background-color:#E9E7E8;border-radius:0.1rem;@include box-sizing;color:#000;
		}
	}
	.jy_bl_select_ico{
		position:relative;margin-top:0.1rem;@extend %oneLine;
		i{margin:0;position:absolute;right:0.08rem;top:0.08rem;}
	}
}

@keyframes text_ani{
	0%{transform:scale(1);}
	100%{transform:scale(1.2);}
}
@-webkit-keyframes text_ani{
	0%{-webkit-transform:scale(1);}
	100%{-webkit-transform:scale(1.2);}
}
.join_text{
	color:#ff788c;padding:0 0.35rem;margin-top:1rem;
}
.jy_zhiyin{
	display:block;width:100%;
}
.jy_introduceList{
	background-color:#fff;font-size:0.5rem;padding:0.2rem 0.3rem 0;
	dt{
		border-bottom:1px solid #f1f1f1;background-color:#f2f2f2;
		i{
			display:block;width:100%;padding-top:39.88%;@include fixbackground;background-image:url(../images/j1.png);
		}
		p{
			padding:0.2rem 0.3rem;	
		}
	}
}
.jy_introduceList_list{
	overflow:hidden;
	li{
		overflow:hidden;border-bottom:1px solid #f1f1f1;line-height:1.2;padding:0.3rem 0;
		i{
			display:block;float:left;width:2.08rem;height:1.82rem;margin-right:0.3rem;@include fixbackground;background-size:auto 100%;
			&.jy_i_list_i1{
				background-image:url(../images/j2.png);
			}
			&.jy_i_list_i2{
				background-image:url(../images/j3.png);
			}
			&.jy_i_list_i3{
				background-image:url(../images/j4.png);
			}
			&.jy_i_list_i4{
				background-image:url(../images/j5.png);
			}
			&.jy_i_list_i5{
				background-image:url(../images/j6.png);
			}
		}
		em{
			color:#bfbfbf;display:block;font-size:0.4rem;line-height:1.2;margin-top:0.2rem;
		}
	}
}
.jy_act_step_btn{
	overflow:hidden;padding:0 0.4rem;
	li{
		position:relative;width:20%;padding:0.1rem;float:left;text-align:center;color:#fff;border-radius:50%;@include box-sizing;@include opacity(50);
		&.on,&.cur{
			@include opacity(100);
		}
		i{
			position:relative;display:block;width:1.08rem;height:1.08rem;margin:0 auto;cursor:pointer;
			&:before{
				content:"";position:absolute;left:0;top:0;width:100%;height:100%;background-position:center center;background-repeat:no-repeat;background-size:auto 0.65rem;
			}
			/*&:active{
				@include opacity(80);
			}*/
			/* 背景颜色*/
			@for $i from 1 through 5 {
				&.bd_btn_bg_#{$i}{
					background:url(../images/btn_bg_#{$i}.png) no-repeat;background-size:100% 100%;
				}
			}
			@for $i from 1 through 13 {
				&.bd_btn_#{$i}{
					&:before{
						background-image:url(../images/btn_#{$i}.png);
					}
				}
			}
			&.cur{
				&:after{
					 content: attr(data-num);position: absolute;right: -0.05rem;top: -0.1rem;width: 0.4rem;height: 0.4rem;border-radius: 50%;background-color: #FE0000;text-align:center;line-height:0.4rem;font-size:0.22rem;
				}
			}
