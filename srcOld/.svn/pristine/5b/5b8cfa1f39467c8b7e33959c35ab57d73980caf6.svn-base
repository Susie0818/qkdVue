@keyframes jy_s1_ani {
	0%{
		transform:translateX(0%);@include opacity(0);
	}
	30%{
		transform:translateX(-30%);@include opacity(100);
	}
	70%{
		transform:translateX(-70%);@include opacity(100);
	}
	100%{
		transform:translateX(-100%);@include opacity(0);
	}
}
@-webkit-keyframes jy_s1_ani {
	0%{
		-webkit-transform:translateX(0%);@include opacity(0);
	}
	30%{
		-webkit-transform:translateX(-30%);@include opacity(100);
	}
	70%{
		-webkit-transform:translateX(-70%);@include opacity(100);
	}
	100%{
		-webkit-transform:translateX(-100%);@include opacity(0);
	}
}
